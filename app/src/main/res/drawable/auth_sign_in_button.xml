<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="false">

    <shape
        >
        <solid android:color="@color/msLabPrimary"></solid>
        <corners android:radius="20dp"/>
        <stroke android:color="@color/msLabWhite"  android:width="2dp"></stroke>
    </shape>


</item>
    <item android:state_pressed="true">

        <shape
            >
            <solid android:color="@color/msLabPrimary"></solid>
            <corners android:radius="20dp"/>
            <stroke android:color="@color/msLabAccent_1"  android:width="3dp"></stroke>
        </shape>


    </item>
</selector>